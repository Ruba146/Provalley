<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project2024.scenecontroller">
   <children>
      <Button fx:id="startbutton" layoutX="223.0" layoutY="22.0" mnemonicParsing="false" onAction="#startButtonAction" text="start" />
      <Label fx:id="title" layoutX="45.0" layoutY="26.0" text="Timer" />
      <Label fx:id="timerlabel" layoutX="124.0" layoutY="26.0" prefHeight="17.0" prefWidth="80.0" />

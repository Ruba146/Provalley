      <Label layoutX="41.0" layoutY="89.0" text="minut" />
      <Label layoutX="39.0" layoutY="114.0" text="seconds" />
      <TextField fx:id="hoursField" layoutX="113.0" layoutY="57.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="20.0" prefWidth="80.0" />
      <TextField fx:id="minutfield" layoutX="113.0" layoutY="85.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="20.0" prefWidth="80.0" />
      <TextField fx:id="secondsField" layoutX="113.0" layoutY="115.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="20.0" prefWidth="80.0" />

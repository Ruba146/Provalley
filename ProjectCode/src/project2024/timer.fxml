      <Button layoutX="175.0" layoutY="149.0" mnemonicParsing="false" onAction="#switchTocafe" text="back to cafe" />
      <Button layoutX="38.0" layoutY="149.0" mnemonicParsing="false" onAction="#switchTolib" text="back to libary" />
   </children>
</AnchorPane>
